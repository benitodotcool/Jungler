<main>

    <div class="main-wrapper">

        <div class="main-head">
            <h1 class="title">Swipe</h1>
        </div>

        <div class="main-content">

            <div class="swipe-wrapper">
                <div class="swipe-cards"><h1>A</h1></div>
                <div class="swipe-cards"><h1>B</h1></div>
                <div class="swipe-cards"><h1>C</h1></div>
                <div class="swipe-cards"><h1>D</h1></div>
                <div class="swipe-cards"><h1>E</h1></div>
            </div>

            <%= @user_select.summoner_name %>
            <%=@user_select.id%>

            <p><%= link_to "NON", matches_path(:requestor_id => current_user.id.to_i,:receiver_id => @user_select.id.to_i, status:false), method: :post,id:'no',remote:true %></p>
            <br>

            <p><%= link_to "OUI ", matches_path(:requestor_id => current_user.id.to_i,:receiver_id => @user_select.id.to_i, status:true), method: :post,id:'yes',remote:true %></p>

        </div>

    </div>

</main>

<aside>
    <%= render "aside" %>
</aside>