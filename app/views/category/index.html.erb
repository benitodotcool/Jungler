<main id="category-focus">

    <div class="main-wrapper">

        <div class="main-head">
            <h1 class="title">Nos Utilisateurs</h1>
        </div>

        <div class="main-content-category">
            <% @users.reverse_each do |user|%>
                <% if user.id != current_user.id%>
                <div class="cell inset">
                    <div class="head">
                        <h4><%= user.summoner_name%></h4>
                        <h5><%= user.user_game_stat.level%></h5>
                    </div>
                    <div class="content">
                        <p><%=user.user_game_stat.primary_role%></p>
                        <p><%=user.user_game_stat.secondary_role%></p>
                    </div>
                </div>
                <%end%>
            <%end%>
        </div>

    </div>

</main>

<aside>
    <%= render "aside" %>
</aside>