<div class="main-wrapper" id="category-focus">
    <div class="main-head">
        <h1 class="title">Communaut√©</h1>
    </div>
    <div class="main-content-category">

        <% @users.reverse_each do |user|%>

            <% if user.id != current_user.id%>

                <%if user.user_game_stat != nil%>

                    <% if user.tag_list == ["Peer-Gaming"] %>

                        <div class="community-cell-peer-gaming inset" data-tilt>

                            <div class="head-community">

                                <div class="icon-user">                                
                                    <img src=<%="https://ddragon.leagueoflegends.com/cdn/11.12.1/img/profileicon/#{user.icon_profile_id}.png"%>>
                                    <h5><%= user.user_game_stat.level%></h5>
                                </div>

                                <div class="content-community">
                                    <h4><%= user.summoner_name %></h4>
                                    <h5><%= user.tag_list %></h5>
                                </div>                            

                            </div>

                            <div class="content">

                                <div class="role" data-tilt>
                                    <% if user.primary_role == "Top-lane" %>
                                        <%= image_tag 'toplane.png'%>
                                    <% elsif user.primary_role == "Mid-lane" %>
                                        <%= image_tag 'midlaner.png'%>
                                    <% elsif user.primary_role == "Bot-lane Support" %>
                                        <%= image_tag 'support.png', class:"small"%>
                                    <% elsif user.primary_role == "Bot-lane ADC" %>
                                        <%= image_tag 'adc.png'%>
                                    <% elsif user.primary_role == "Jungle" %>
                                        <%= image_tag 'jungler.png', class:"small"%>            
                                    <% end %>
                                </div>

                                <div class="role" data-tilt>
                                    <% if user.secondary_role == "Top-lane" %>
                                        <%= image_tag 'toplane.png'%>
                                    <% elsif user.secondary_role == "Mid-lane" %>
                                        <%= image_tag 'midlaner.png'%>
