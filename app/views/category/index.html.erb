                                    <% elsif user.secondary_role == "Jungle" %>
                                        <%= image_tag 'jungler.png', class:"small"%>            
                                    <% end %>
                                </div>
                                
                            </div>

                        </div>

                    <% end %>

                    <% if user.tag_list == ["Relax"] %>

                        <div class="community-cell-relax inset" data-tilt>

                            <div class="head-community">

